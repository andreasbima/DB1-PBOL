<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="contohdb1.FXMLDocumentController">
    <children>
      <MenuBar layoutY="2.0">
        <menus>
          <Menu mnemonicParsing="false" text="Master">
            <items>
              <MenuItem fx:id="mastersiswa" mnemonicParsing="false" onAction="#mastersiswaklik" text="Siswa" />
                  <MenuItem fx:id="mastermatkul" mnemonicParsing="false" onAction="#mastermatkulklik" text="Mata Kuliah" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Transaksi">
            <items>
              <MenuItem fx:id="transaksinilai" mnemonicParsing="false" onAction="#transaksinilaiklik" text="Penilaian" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Display">
            <items>
              <MenuItem fx:id="displaysiswa" mnemonicParsing="false" onAction="#displaysiswaklik" text="Siswa" />
                  <MenuItem fx:id="displaymatkul" mnemonicParsing="false" onAction="#displaymatkulklik" text="Mata Kuliah" />
                  <MenuItem fx:id="displaynilai" mnemonicParsing="false" onAction="#displaynilaiklik" text="Penilaian" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Keluar">
              <items>
                <MenuItem fx:id="keluar" mnemonicParsing="false" onAction="#keluarklik" text="Keluar" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
    </children>
</AnchorPane>
